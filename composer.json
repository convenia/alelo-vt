{
    "name": "convenia/alelo-vt",
    "description": "Este package viabiliza a criação de pedidos dos integração de Vale Transporte ALELO em arquivos “TEXTO” que podem ser gerados a partir da Folha de Pagamento de sua Empresa.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "alelo",
        "VT",
        "Vale Transporte",
        "Integração"
    ],
    "authors": [
        {
            "name": "Eduardo Bizarro",
            "email": "edbizarro@gmail.com"
        },
        {
            "name": "Victor Ventura",
            "email": "euventura@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "danielstjules/stringy": "^2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0|^5.0",
        "fabpot/php-cs-fixer": "^1.11",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "Convenia\\AleloVt\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Convenia\\AleloVt\\Tests\\": "tests/"
        }
    },
    "config":{
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
